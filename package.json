{
  "name": "easy-node-proxy",
  "version": "1.2.1",
  "description": "The nodejs proxy server that likes nginx help you resolve cors.",
  "main": "index.js",
  "scripts": {
    "test": "node test.js",
    "dev": "nodemon",
    "start": "node ./src/index.js",
    "prod": "(pm2 delete all || true) && pm2 start src/index.js -i 2 ",
    "stop": "pm2 stop all",
    "generate:config": "node ./scripts/setupConfig.js"
  },
  "bin": {
    "easy-node-proxy": "./bin/easy-node-proxy"
  },
  "author": "chenkang@084@163.com",
  "license": "ISC",
  "dependencies": {
    "chalk": "^2.4.1",
    "colors": "1.0.3",
    "commander": "^2.18.0",
    "compression": "^1.7.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "http-proxy-middleware": "^0.19.0",
    "optimist": "0.6.x",
    "pm2": "^3.0.3",
    "shelljs": "^0.8.2"
  },
  "devDependencies": {
    "nodemon": "^1.18.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chenkang084/easy-node-proxy.git"
  }
}
